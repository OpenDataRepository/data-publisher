{% spaceless %}
<div class="pure-u-1-5">
    <div style="margin: 20px; border: 1px solid black">
        <div class="pure-u-1-2" style="text-align: center;">
            <div class="title">{{ datatype_name }}</div>
            <div class="total green">{{ created_str }}</div>
            <div class="total red">{{ updated_str }}</div>
        </div>
        <div class="pure-u-1-2" style="text-align: center; position:relative; top:15px;">
            <div class="sparkline" values="{{ value_str }}"></div>
            {#<div class="change {% if datatype.change > 0 %}green{% elseif datatype.change < 0 %}red{% endif %}">{% if datatype.change > 0 %}+{% endif %}{{ datatype.change }}%</div>#}
        </div>
    </div>
</div>
{% endspaceless %}

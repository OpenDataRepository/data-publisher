{% set tag_meta = tag.tagMeta %}

{
    "id": {{ tag.id }},
    "template_tag_uuid": {{ tag.tagUuid|json_encode|raw }},
    {% if tag.parent_tagUuid is defined %}
        "parent_template_tag_uuid": {{ tag.parent_tagUuid|json_encode|raw }},
    {% endif %}
    "name": "{{ tag.tagName }}",
    {% if tag.userCreated %}
        "user_created": "1",
    {% endif %}
    "tag_created": "{{ tag.created|date('Y-m-d H:i:s') }}",
    "tag_selected": "{{ tag_selection.created|date('Y-m-d H:i:s') }}",
    "selected": 1
},

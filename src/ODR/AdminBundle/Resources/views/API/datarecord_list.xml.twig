{% spaceless %}

<?xml version="1.0" encoding="UTF-8"?>
<records>
{% for num, datarecord in datarecord_list.records %}
    <record>
        <internal_id>{{ datarecord.internal_id }}</internal_id>
        <external_id>{{ datarecord.external_id }}</external_id>
        <record_name>{{ datarecord.record_name }}</record_name>
    </record>
{% endfor %}
</records>

{% endspaceless %}

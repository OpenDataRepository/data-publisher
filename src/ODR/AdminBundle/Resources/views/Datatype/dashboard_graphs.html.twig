{% spaceless %}
    <div class="ODRTableOuterWrap pure-u-1 pure-u-md-8-24">
        <div class="pure-u-1 ODRTableWrap">
            <div>
                <div class="pure-u-1 title">{{ datatype_name }}</div>
                <div class="pure-u-1-2" style="text-align: center;">
                    <div class="total ODRSparklineGreen">{{ created_str }}</div>
                    <div class="total ODRSparklineRed">{{ updated_str }}</div>
                </div>
                <div class="pure-u-1-2">
                    <div class="pure-u-1 sparkline" values="{{ value_str }}"></div>
                </div>
            </div>
        </div>
    </div>
{% endspaceless %}

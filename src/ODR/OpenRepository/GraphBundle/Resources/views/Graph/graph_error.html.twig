    <?xml version="1.0" standalone="no"?>
    <!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.1//EN"
            "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd">
    <svg preserveAspectRatio="xMinyMin meet" viewBox="0 0 1350 450" width="100%" height="100%" version="1.1" xmlns="http://www.w3.org/2000/svg">
    <style type="text/css">
        <![CDATA[
        #message {
            fill: rgb(184, 232, 250);
            font-family: Arial, sans-serif;
        }
        #detail {
            fill: rgb(184, 232, 250);
            font-family: Arial, sans-serif;
        }
        rect {
            fill: white;
            stroke: rgb(184, 232, 250);
            stroke-width: 4;
            stroke-opacity: 0.9;
        }
        ]]>
    </style>

        <rect width="1300" height="400" x="25" y="25" rx="20" ry="20" />

        <text id="message" x="50%" y="50%" font-size="32" text-anchor="middle">{{ message }}</text>
        <text id="detail" x="50%" y="80%" font-size="20" text-anchor="right">{{ detail }}</text>

    </svg>
{% endspaceless %}
